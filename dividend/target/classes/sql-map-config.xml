<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- Alias 설정 -->
	<typeAliases>
		<typeAlias alias="dividend" type="com.baedang.biz.dividend.DividendVO"/>
		<typeAlias alias="member" type="com.baedang.biz.member.MemberVO"/>
		<typeAlias alias="cart" type="com.baedang.biz.cart.CartVO"/>
		<typeAlias alias="freeboard" type="com.baedang.biz.freeboard.FreeboardVO"/>
		<typeAlias alias="reple" type="com.baedang.biz.freeboard.RepleVO"/>
	</typeAliases>
	
	<!-- Sql Mapper 설정 -->
	<mappers>
		<mapper resource="mappings/dividend-mapping.xml"/>
 		<mapper resource="mappings/member-mapping.xml"/>
 		<mapper resource="mappings/cart-mapping.xml"/>
 		<mapper resource="mappings/freeboard-mapping.xml"/>
	</mappers>
</configuration>